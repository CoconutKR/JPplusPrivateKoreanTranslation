switch (FEAT_TRAINS, SELF, tk_hb_13000series_articulated_part, extra_callback_info1){
0 .. 2: return tk_hb_13000series;
}
switch (FEAT_TRAINS, SELF, tk_hb_13000series_mucar, extra_callback_info1){
0 .. 2: return mu_car;
}

switch(FEAT_TRAINS, SELF, tk_hb_13000series_main,position_in_consist){
    1: tk_hb_13000series_front;
    default: dummy_sprites;
}
