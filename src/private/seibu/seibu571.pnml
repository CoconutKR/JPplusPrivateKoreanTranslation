          2: seibu571_mot;
          seibu571_wag;
      }
      switch(FEAT_TRAINS, SELF, sw_seibu571_panto_placer_rev,position_in_vehid_chain_from_end %4 -1){
          2: seibu571_mot_rev;
          seibu571_wag;
