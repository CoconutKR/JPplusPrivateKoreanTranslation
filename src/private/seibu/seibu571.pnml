  //capacity fix
    switch(FEAT_TRAINS, SELF, sw_seibu571_capacity,position_in_vehid_chain_from_end){
        0: return 160;
        return 168;
    }
  //power fix
    switch(FEAT_TRAINS, SELF, sw_seibu571_power,position_in_vehid_chain_from_end %2){
      0: return 0;
      return 400*1342/1000;
    }
